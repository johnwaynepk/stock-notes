# Database
DATABASE_URL="postgresql://user:password@localhost:5432/stockwatchlist?schema=public"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-generate-with-openssl-rand-base64-32"

# Market Data Provider
MARKET_DATA_PROVIDER="alpha_vantage" # alpha_vantage | twelve_data | finnhub | mock
ALPHA_VANTAGE_API_KEY="your-alpha-vantage-api-key"
TWELVE_DATA_API_KEY="your-twelve-data-api-key"
FINNHUB_API_KEY="your-finnhub-api-key"

# Optional: Fallback provider
FALLBACK_PROVIDER="mock"

# Rate Limiting
RATE_LIMIT_SEARCH=10 # per minute per user
RATE_LIMIT_QUOTES=100 # per hour per user

# Cache TTL (seconds)
CACHE_SEARCH_TTL=3600
CACHE_QUOTE_TTL=300
CACHE_HISTORICAL_TTL=86400
